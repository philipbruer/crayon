  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5d656e"
      }
    },
    {
      "name": "Punctuations",
      "scope": ["punctuation.definition"],
      "settings": {
        "foreground": "#8f98a1"
      }
    },
    {
      "name": "Misc",
      "scope": [
        "meta.object-literal.key",
        "punctuation.section.embedded",
        "support.variable"
      ],
      "settings": {
        "foreground": "#f9f9f9"
      }
    },
    {
      "name": "Tag, Operator",
      "scope": [
        "entity.name.tag",
        "keyword.control.as",
        "keyword.operator",
        "punctuation.definition.keyword",
        "punctuation.definition.template-expression",
        "punctuation.separator.key-value",
        "storage.type.function.arrow",
        "support.class.component",
        "variable.language"
      ],
      "settings": {
        "foreground": "#ff6188"
      }
    },
    {
      "name": "String",
      "scope": ["markup.inline", "string"],
      "settings": {
        "foreground": "#fcad83"
      }
    },
    {
      "name": "Class, Constant, Types",
      "scope": [
        "entity.name.type",
        "support.class",
        "meta.class",
        "meta.function-call",
        "punctuation.definition.entity",
        "constant"
      ],
      "settings": {
        "foreground": "#f7e87e"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "meta.definition.variable",
        "variable.other",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#19f9d8"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.tag.attribute",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#6fc1ff"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "entity.other.attribute-name",
        "keyword",
        "storage",
        "storage.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#c594c5"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": [
        "comment",
        "markup.italic",
        "punctuation.definition.italic",
        "storage.type",
        "support.type.builtin",
        "support.type.primitive",
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
