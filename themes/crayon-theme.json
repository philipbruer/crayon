{
  "name": "Crayon Theme",
  "type": "dark",
  "colors": {
    // Integrated Terminal Colors
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_integrated-terminal-colors
    "terminal.ansiBlack": "#939293",
    "terminal.ansiBrightBlack": "#939293",
    "terminal.ansiWhite": "#f9f9f9",
    "terminal.ansiBrightWhite": "#f9f9f9",
    "terminal.ansiRed": "#ff6188",
    "terminal.ansiBrightRed": "#ff6188",
    "terminal.ansiYellow": "#fc9867",
    "terminal.ansiBrightYellow": "#fc9867",
    "terminal.ansiGreen": "#f7e87e",
    "terminal.ansiBrightGreen": "#f7e87e",
    "terminal.ansiBlue": "#6fc1ff",
    "terminal.ansiBrightBlue": "#6fc1ff",
    "terminal.ansiCyan": "#19f9d8",
    "terminal.ansiBrightCyan": "#19f9d8",
    "terminal.ansiMagenta": "#c594c5",
    "terminal.ansiBrightMagenta": "#c594c5",
    "terminal.border": "#30373A",

    // Editor Colors
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_editor-colors
    "editor.background": "#292a2b",
    "editor.foreground": "#f9f9f9"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5f6672"
      }
    },
    {
      "name": "Punctuations",
      "scope": ["punctuation.definition"],
      "settings": {
        "foreground": "#939293"
      }
    },
    {
      "name": "Misc",
      "scope": [
        "meta.object-literal.key",
        "punctuation.section.embedded",
        "support.type.property-name",
        "support.variable"
      ],
      "settings": {
        "foreground": "#f9f9f9"
      }
    },
    {
      "name": "Tag, Operator",
      "scope": [
        "entity.name.tag",
        "keyword.control.as",
        "keyword.operator",
        "punctuation.definition.keyword",
        "punctuation.definition.template-expression",
        "punctuation.separator.key-value",
        "storage.type.function.arrow",
        "support.class.component",
        "variable.language"
      ],
      "settings": {
        "foreground": "#ff6188"
      }
    },
    {
      "name": "String",
      "scope": ["markup.inline", "string"],
      "settings": {
        "foreground": "#fc9867"
      }
    },
    {
      "name": "Class, Constant, Types",
      "scope": [
        "entity.name.type",
        "support.class",
        "meta.class",
        "meta.function-call",
        "punctuation.definition.entity",
        "constant"
      ],
      "settings": {
        "foreground": "#f7e87e"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.tag.attribute",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#6fc1ff"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "meta.definition.variable",
        "variable.other",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#19f9d8"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "entity.other.attribute-name",
        "keyword",
        "storage",
        "storage.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#c594c5"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": [
        "comment",
        "markup.italic",
        "punctuation.definition.italic",
        "storage.type",
        "support.type.builtin",
        "support.type.primitive",
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
